/* Product sticky add to cart
-----------------------------------------------------*/
.main-product-sticky-add-to-cart {
  position: fixed;
  left: 0;
  bottom: 0;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  height: 85px;
  padding: 0 10px;
  background-color: #fff;
  border: 1px solid #DDDDDD;
  z-index: 8; // smaller than mini cart
  
  &.hide {
  	display: none;
  }
  
  @media (min-width: 768px) {
  	display: none;
  }
  
  img {
    -webkit-box-flex: 0;
     -ms-flex-positive: 0;
     flex-grow: 0;
     -ms-flex-negative: 0;
     flex-shrink: 0;
     -ms-flex-preferred-size: auto;
     flex-basis: auto;
    max-height: 53.34px;
  	margin-right: 10px;
    border: 1px solid #DDDDDD;
  }
  
  span.hide {
    display: none;
  }
  
  .main-product #pre-order {font-family: "HelveticaNeueLTStd-BdEx",sans-serif !important;width:100%;margin-bottom:0;letter-spacing: 0.05em;font-size: 16px !important;line-height: 24px;border-radius: 5px;}
  .button {
    -webkit-box-flex: 1;
    -ms-flex-positive: 1;
    flex-grow: 1;
    -ms-flex-negative: 1;
    flex-shrink: 1;
    -ms-flex-preferred-size: auto;
    flex-basis: auto;
    height: 65px;
    padding: 0;
    margin: 0;
    cursor: pointer;
    font-size: 15px;
    line-height: 65px;
    letter-spacing: 0.05em;
  }
  
  .price {
  	font-weight: 300;
  }
  
  .product-compare-price {
  	margin-left: 5px;
    text-decoration: line-through;
  }

  // variants dropdown
  .sticky-variants {
    position: relative;
    display: inline-block;
    margin-right: 10px;
  }

  .sticky-active-variant {
    padding: 0 20px;
    cursor: pointer;
    background-color: #ddd;
    color: #000;
    border-color: #ddd;
    font-weight: normal;

    &:hover {
      background-color: #ddd;
      opacity: 0.6;
    }

    svg {
      display: inline-block;
      width: 8px;
    }
  }

  .sticky-variants-dropdown {
    display: none;
    position: absolute;
    left: 0;
    bottom: 100%;
    width: 100%;
    margin: 0;
    border: 1px solid #000;
    cursor: pointer;
    font-family: Helvetica,Arial,sans-serif;
    background-color: transparentize(#fff, 0.25);
    
    li {
      padding: 6px 0;
    }

    li:not(:first-child) {
      border-top: 1px solid;
    }

    li:hover {
      background-color: #000;
      color: #fff;
    }
  }
  
  .back-to-top {
    display: none;
  	position: absolute;
    top: -100px;
    right: 20px;
    width: 60px;
    height: 60px;
    border-radius: 50%;
    background-color: #dddddd;
    font-size: 0;
    cursor: pointer;
    @extend .transitions;
    
    &:hover {
      background-color: transparentize(#dddddd, 0.4);
    }
    
    &::before {
      content: '';
      display: inline-block;
      height: 100%;
      vertical-align: middle;
    }
    
    svg {
      display: inline-block;
      height: 20px;
    }
  }
}

