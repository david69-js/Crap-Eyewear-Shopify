  position: relative;
  height: 100%;
}


.cart-drawer .icon-wrapper {
  display: inline-block;
  font-size: 0;
  vertical-align: middle;
  
/*   &:hover,
  &:focus {
    opacity: 0.6;
  } */
}

.cart-drawer a,
