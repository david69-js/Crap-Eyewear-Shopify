/* SCSS
---------------------------------------------------------*/
* {
  box-sizing: border-box;
}
.srt {
  border: 0;
  clip: rect(0 0 0 0);
  height: 1px;
  margin: -1px;
  overflow: hidden;
  padding: 0;
  position: absolute;
  width: 1px;
}
@mixin img-replace($img, $w, $h, $disp: block) {
  background-image: url('#{$img}');
  background-repeat: no-repeat;
  width: $w;
  height: $h;
  display: $disp;
}
@mixin keyframes($name) {
  @-webkit-keyframes #{$name} {
    @content;
  }
  @-moz-keyframes #{$name} {
    @content;
  }
  @-ms-keyframes #{$name} {
    @content;
  }
  @keyframes #{$name} {
    @content;
  }
}
.clearfix {
  &:after {
    content: "";
    display: table;
    clear: both;
  }
}
.clear {
  clear: both;
}
a {
  text-decoration: none;
}
img {
  max-width: 100%;
}
input {
  border-radius: 0;
  -webkit-appearance: none !important;
}
/* Drawer
---------------------------------------------------------*/
.drawer {
  position: fixed;
  display: none;
  width: 81%;
  transition: all 0.4s cubic-bezier(0.46, 0.01, 0.32, 1) 0s;
  overflow: hidden;
  -webkit-overflow-scrolling: touch;
  z-index: 999999;
  -webkit-box-shadow: 0px 2px 16px -2px rgba(121, 121, 121, 0.1);
          box-shadow: 0px 2px 16px -2px rgba(121, 121, 121, 0.1);

  input[type="text"],
  textarea {
    background-color: #fff;
    color: #000;
  }
}

/** 
 * This prevents the docuent body from scrolling
 * This also makes the body wider, because the scroll bar disappears
 */
.drawer--top {
  top: 0;
  right: 0;
  width: 100%;
  opacity: 0;
  -webkit-transform: translate3d(0, -100%, 0);
          transform: translate3d(0, -100%, 0);
  // Transition drawer but a little bit slower
  -webkit-transition: all 0.6s cubic-bezier(0.29, 0.63, 0.44, 1);
  transition: all 0.6s cubic-bezier(0.29, 0.63, 0.44, 1);

  .js-drawer-open-top & {
    /** 
     * If we use transform here, all fixed child inside drawer--left will not
     * be relative to the view port
     * https://stackoverflow.com/questions/15194313/transform3d-not-working-with-position-fixed-children
     */
	-webkit-transform: translate3d(0, 0, 0);
	        transform: translate3d(0, 0, 0);
    display: block;
    opacity: 1;
  }
}

.drawer--left {
  left: 0;
  bottom:0;
  opacity: 0;
  -webkit-transform: translate3d(100%, 0, 0);
          transform: translate3d(100%, 0, 0);
  // Transition drawer
  -webkit-transition: opacity 0.2s, -webkit-transform 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);
  transition: opacity 0.2s, -webkit-transform 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);
  transition: opacity 0.2s, transform 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);
  transition: opacity 0.2s, transform 0.45s cubic-bezier(0.29, 0.63, 0.44, 1), -webkit-transform 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);

  .js-drawer-open-left & {
    /** 
     * If we use transform here, all fixed child inside drawer--left will not
     * be relative to the view port
     * https://stackoverflow.com/questions/15194313/transform3d-not-working-with-position-fixed-children
     */
    -webkit-transform: translate3d(0, 0, 0);
            transform: translate3d(0, 0, 0);
    display: block;
    opacity: 1;
  }
}

.drawer--right {
  top: 0;
  bottom: 0;
  right: 0;
  opacity: 0;
  -webkit-transform: translate3d(100%, 0, 0);
          transform: translate3d(100%, 0, 0);
  // Transition drawer
  -webkit-transition: opacity 0.2s, -webkit-transform 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);
  transition: opacity 0.2s, -webkit-transform 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);
  transition: opacity 0.2s, transform 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);
  transition: opacity 0.2s, transform 0.45s cubic-bezier(0.29, 0.63, 0.44, 1), -webkit-transform 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);

  .js-drawer-open-right & {
    /** 
     * If we use transform here, all fixed child inside drawer--left will not
     * be relative to the view port
     * https://stackoverflow.com/questions/15194313/transform3d-not-working-with-position-fixed-children
     */
    -webkit-transform: translate3d(0, 0, 0);
            transform: translate3d(0, 0, 0);
    display: block;
    opacity: 1;
  }
}

.is-transitioning {
  display: block !important;
  visibility: visible !important;
}

#page-container::after {
  visibility: hidden;
  opacity: 0;
  content: "";
  display: block;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.85);
  z-index: 150;
  -webkit-transition: all 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);
  transition: all 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);
}
.js-drawer-open {
  height: 100%;
}
.js-drawer-open #page-container::after {
  visibility: visible;
  opacity: 1;
}
.js-drawer-open-left.js-drawer-open #page-container::after {visibility: hidden;}
